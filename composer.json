{
    "name": "vahidirn/neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database Graph interface",
    "keywords": [
        "laravel",
        "neo4j",
        "graph",
        "database",
        "neoeloquent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vahid Amiri",
            "email": "vahidamiri251@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.*",
        "illuminate/database": "5.*",
        "illuminate/pagination": "5.*",
        "illuminate/events": "5.*",
        "illuminate/console": "5.*",
        "heydavid713/neo4jphp": "0.1.*",
        "nesbot/carbon": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "1.0"
    },
    "repositories": [
    ],
    "autoload": {
        "psr-4": {
            "VahidIrn\\NeoEloquent\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "VahidIrn\\NeoEloquent\\NeoEloquentServiceProvider"
            ]
        }
    }
}
